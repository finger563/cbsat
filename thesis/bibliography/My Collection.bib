issn = {1095-323X},
journal = {2012 IEEE Aerospace Conference},
pages = {1--20},
publisher = {Ieee},
title = {{A software platform for fractionated spacecraft}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=6187334},
year = {2012}
}
