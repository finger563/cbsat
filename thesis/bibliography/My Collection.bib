booktitle = {IEEE International Conference on Communications},
doi = {10.1109/ICC.2010.5502376},
file = {:C$\backslash$:/Users/William/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ciucu, Hohlfeld - 2010 - On computing bounds on average backlogs and delays with network calculus.pdf:pdf},
